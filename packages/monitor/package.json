{
  "name": "@cmtlyt/monitor",
  "type": "module",
  "version": "0.1.0",
  "private": false,
  "packageManager": "pnpm@9.14.4",
  "description": "cl项目监控",
  "author": {
    "name": "cmtlyt",
    "email": "cmtlyt@163.com"
  },
  "license": "MIT",
